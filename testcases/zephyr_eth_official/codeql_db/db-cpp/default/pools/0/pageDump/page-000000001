 * @since 1.0
 * @version 1.1.0
 * @{
 */dev_handledevices<zephyr/device.h>z_irq_spurious_isr_wrapper_isr_list_sname_isr_list_irq_parent_entry_isr_table_entrysnameparamoffsetlevelisrconst _isr_table_entryconst _isr_table_entry[]_isr_table_entry[]_sw_isr_tableIRQ_TABLE_SIZE(CONFIG_NUM_IRQS - CONFIG_GEN_IRQ_START_VECTOR)Z_ISR_DECLARE_DIRECT(irq,flags,func)Z_ISR_DECLARE(irq, ISR_FLAG_DIRECT | (flags), func, NULL)Z_ISR_DECLARE(irq,flags,func,param)static Z_DECL_ALIGN(struct _isr_list) Z_GENERIC_SECTION(.intList) __used _MK_ISR_NAME(func, __COUNTER__) = {irq, flags, (void *)&func, (const void *)param}__MK_ISR_NAME(x,y)__isr_ ## x ## _irq_ ## y_MK_ISR_NAME(x,y)__MK_ISR_NAME(x, y)ISR_FLAG_DIRECTIRQ_PARENT_ENTRY_DEFINE(_name,_dev,_irq,_offset,_level)static const STRUCT_SECTION_ITERABLE_ALTERNATE(intc_table, _irq_parent_entry, _name) = { .dev = _dev, .level = _level, .irq = _irq, .offset = _offset, }INTC_CHILD_ISR_TBL_OFFSET(node_id)(INTC_BASE_ISR_TBL_OFFSET(node_id) + (DT_NODE_CHILD_IDX(node_id) * CONFIG_MAX_IRQ_PER_AGGREGATOR))INTC_INST_ISR_TBL_OFFSET(inst)(INTC_BASE_ISR_TBL_OFFSET(DT_DRV_INST(inst)) + (inst * CONFIG_MAX_IRQ_PER_AGGREGATOR))INTC_BASE_ISR_TBL_OFFSET(node_id)Z_SW_ISR_TBL_KCONFIG_BY_ALVL(DT_INTC_GET_AGGREGATOR_LEVEL(node_id))Z_SW_ISR_TBL_KCONFIG_BY_ALVL(l)CONCAT(CONFIG_, CONCAT(Z_STR_L, l), _LVL_ISR_TBL_OFFSET)Z_STR_L33RDZ_STR_L22NDZEPHYR_INCLUDE_SW_ISR_TABLE_H_defined(CONFIG_ISR_TABLES_LOCAL_DECLARATION)/* ZEPHYR_INCLUDE_SW_ISR_TABLE_H_ *//* The version of the Z_ISR_DECLARE that should be used for direct ISR declaration.
